Notebook[{

Cell[CellGroupData[{
Cell["Solution to the Question 1", "Title",
 CellChangeTimes->{{3.819682936938311*^9, 3.8196829461605434`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"A", "=", 
    RowBox[{"(", GridBox[{
       {"4", "6", "6"},
       {"1", "3", "2"},
       {
        RowBox[{"-", "1"}], 
        RowBox[{"-", "4"}], 
        RowBox[{"-", "3"}]}
      }], ")"}]}], ";"}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.819682804808636*^9, 3.819682916829783*^9}, {
   3.8196829748396606`*^9, 3.819682976607012*^9}, 3.8196831329282856`*^9}],

Cell[CellGroupData[{

Cell["By solving equation", "Chapter",
 CellChangeTimes->{{3.819683155920019*^9, 3.819683168599124*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"ev", "=", 
  RowBox[{"NSolve", "[", 
   RowBox[{
    RowBox[{"Det", "[", 
     RowBox[{"A", "-", 
      RowBox[{"\[Lambda]", "*", 
       RowBox[{"IdentityMatrix", "[", 
        RowBox[{"Length", "[", "A", "]"}], "]"}]}]}], "]"}], "\[Equal]", 
    "0"}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\[Lambda]", "\[Rule]", 
     RowBox[{"-", "1.`"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Lambda]", "\[Rule]", "1.`"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Lambda]", "\[Rule]", "4.`"}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{3.8196831752725906`*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Print", "[", 
  RowBox[{"\"\<hence the eigenvalues of A are \>\"", ",", 
   RowBox[{"ev", "[", 
    RowBox[{"[", 
     RowBox[{"1", ",", "1", ",", "2"}], "]"}], "]"}], ",", "\"\<, \>\"", ",", 
   RowBox[{"ev", "[", 
    RowBox[{"[", 
     RowBox[{"2", ",", "1", ",", "2"}], "]"}], "]"}], ",", "\"\< and \>\"", 
   ",", 
   RowBox[{"ev", "[", 
    RowBox[{"[", 
     RowBox[{"3", ",", "1", ",", "2"}], "]"}], "]"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.819682985013129*^9, 3.819683106089017*^9}}],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"hence the eigenvalues of A are \"\>", "\[InvisibleSpace]", 
   RowBox[{"-", "1.`"}], "\[InvisibleSpace]", "\<\", \"\>", 
   "\[InvisibleSpace]", "1.`", "\[InvisibleSpace]", "\<\" and \"\>", 
   "\[InvisibleSpace]", "4.`"}],
  SequenceForm["hence the eigenvalues of A are ", -1., ", ", 1., " and ", 4.],
  Editable->False]], "Print",
 CellChangeTimes->{{3.8196830906883597`*^9, 3.819683106508503*^9}}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["By using command", "Chapter",
 CellChangeTimes->{{3.8196831887878833`*^9, 3.8196831972050495`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"ev1", "=", 
  RowBox[{"Eigenvalues", "[", "A", "]"}]}]], "Input",
 CellChangeTimes->{{3.819683219094113*^9, 3.819683231105149*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"4", ",", 
   RowBox[{"-", "1"}], ",", "1"}], "}"}]], "Output",
 CellChangeTimes->{3.8196832339924707`*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Print", "[", 
  RowBox[{"\"\<hence the eigenvalues of A are \>\"", ",", 
   RowBox[{"ev1", "[", 
    RowBox[{"[", "1", "]"}], "]"}], ",", "\"\<, \>\"", ",", 
   RowBox[{"ev1", "[", 
    RowBox[{"[", "2", "]"}], "]"}], ",", "\"\< and \>\"", ",", 
   RowBox[{"ev1", "[", 
    RowBox[{"[", "3", "]"}], "]"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.81968329576108*^9, 3.8196833404670477`*^9}}],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"hence the eigenvalues of A are \"\>", "\[InvisibleSpace]", "4",
    "\[InvisibleSpace]", "\<\", \"\>", "\[InvisibleSpace]", 
   RowBox[{"-", "1"}], "\[InvisibleSpace]", "\<\" and \"\>", 
   "\[InvisibleSpace]", "1"}],
  SequenceForm["hence the eigenvalues of A are ", 4, ", ", -1, " and ", 1],
  Editable->False]], "Print",
 CellChangeTimes->{3.8196833439729776`*^9}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Verification", "Chapter",
 CellChangeTimes->{{3.81968337364456*^9, 3.819683380668501*^9}}],

Cell["\<\
Cayley-Hamilton Theorem: Every Square matrix satisfies it\[CloseCurlyQuote]s \
Chareacteristics equation\
\>", "Text",
 CellChangeTimes->{{3.819683400852812*^9, 3.8196834744750223`*^9}, {
  3.819684236085746*^9, 3.8196842458094864`*^9}}],

Cell[CellGroupData[{

Cell["Chararterics equation", "Subchapter",
 CellChangeTimes->{{3.8196842241470594`*^9, 3.819684255683407*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Print", "[", 
  RowBox[{
   RowBox[{"CharacteristicPolynomial", "[", 
    RowBox[{"A", ",", "x"}], "]"}], ",", "\"\<=0\>\""}], "]"}]], "Input",
 CellChangeTimes->{{3.8196842613720875`*^9, 3.819684327558771*^9}}],

Cell[BoxData[
 InterpretationBox[
  RowBox[{
   RowBox[{
    RowBox[{"-", "4"}], "+", "x", "+", 
    RowBox[{"4", " ", 
     SuperscriptBox["x", "2"]}], "-", 
    SuperscriptBox["x", "3"]}], "\[InvisibleSpace]", "\<\"=0\"\>"}],
  SequenceForm[-4 + $CellContext`x + 4 $CellContext`x^2 - $CellContext`x^3, 
   "=0"],
  Editable->False]], "Print",
 CellChangeTimes->{3.8196843287673383`*^9}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
Replacing x with matrix A from left side of Characteristics equation and \
Comparing it with Zero matrix\
\>", "Subchapter",
 CellChangeTimes->{{3.819684352075555*^9, 3.8196844164228773`*^9}, {
  3.819684528745019*^9, 3.8196845560491304`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"-", "4"}], "*", 
    RowBox[{"IdentityMatrix", "[", 
     RowBox[{"Length", "[", "A", "]"}], "]"}]}], "+", "A", "+", 
   RowBox[{"4", "*", 
    RowBox[{"MatrixPower", "[", 
     RowBox[{"A", ",", "2"}], "]"}]}], "-", 
   RowBox[{"MatrixPower", "[", 
    RowBox[{"A", ",", "3"}], "]"}]}], "\[Equal]", 
  RowBox[{"(", GridBox[{
     {"0", "0", "0"},
     {"0", "0", "0"},
     {"0", "0", "0"}
    }], ")"}]}]], "Input",
 CellChangeTimes->{{3.819684419974622*^9, 3.8196844861727486`*^9}, {
  3.819684559850171*^9, 3.8196845933151608`*^9}}],

Cell[BoxData["True"], "Output",
 CellChangeTimes->{3.819684502965603*^9, 3.8196845957349873`*^9}]
}, Open  ]],

Cell["hence the theorem is varified", "Text",
 CellChangeTimes->{{3.8196846085463214`*^9, 3.819684619806217*^9}}]
}, Open  ]]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Answer to the Question 2", "Title",
 CellChangeTimes->{{3.81968465037897*^9, 3.8196846642388363`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], "=", 
   RowBox[{
    SuperscriptBox["x", "3"], "-", 
    RowBox[{"3", "*", 
     SuperscriptBox["x", "2"]}], "+", "1"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.819684700984488*^9, 3.819684732085043*^9}}],

Cell[CellGroupData[{

Cell["Part (i)", "Chapter",
 CellChangeTimes->{{3.819684743898874*^9, 3.81968474605606*^9}, {
  3.8196848031119146`*^9, 3.8196848047428603`*^9}}],

Cell[CellGroupData[{

Cell["Critcal points and Points of inflection", "Subchapter",
 CellChangeTimes->{{3.819684848946355*^9, 3.8196848702601404`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Print", "[", 
  RowBox[{"\"\<the critical points are \>\"", ",", 
   RowBox[{"NSolve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "'"}], "[", "x", "]"}], "\[Equal]", "0"}], "]"}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<the inflection points are \>\"", ",", 
   RowBox[{"NSolve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "''"}], "[", "x", "]"}], "\[Equal]", "0"}], "]"}]}], 
  "]"}]}], "Input",
 CellChangeTimes->{{3.819684884366255*^9, 3.8196849516846695`*^9}, {
  3.8196852125039253`*^9, 3.81968529360085*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"the critical points are \"\>", "\[InvisibleSpace]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x", "\[Rule]", "0.`"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", "\[Rule]", "2.`"}], "}"}]}], "}"}]}],
  SequenceForm[
  "the critical points are ", {{$CellContext`x -> 0.}, {$CellContext`x -> 
     2.}}],
  Editable->False]], "Print",
 CellChangeTimes->{3.8196852942230854`*^9}],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"the inflection points are \"\>", "\[InvisibleSpace]", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"x", "\[Rule]", "1.`"}], "}"}], "}"}]}],
  SequenceForm["the inflection points are ", {{$CellContext`x -> 1.}}],
  Editable->False]], "Print",
 CellChangeTimes->{3.8196852942284417`*^9}]
}, Open  ]]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Testing Increasing and Decreasing", "Subchapter",
 CellChangeTimes->{{3.8196849837429924`*^9, 3.819685002927359*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"f", "'"}], "[", 
    RowBox[{"-", "1"}], "]"}], ",", 
   RowBox[{
    RowBox[{"f", "'"}], "[", "1", "]"}], ",", 
   RowBox[{
    RowBox[{"f", "'"}], "[", "3", "]"}]}], "}"}]], "Input",
 CellChangeTimes->{{3.8196850677390947`*^9, 3.8196851241212163`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"9", ",", 
   RowBox[{"-", "3"}], ",", "9"}], "}"}]], "Output",
 CellChangeTimes->{{3.8196850952763295`*^9, 3.819685124827594*^9}}]
}, Open  ]],

Cell["\<\
Hence the function is increasing on the open intervals (-\[Infinity],0) and \
(2,\[Infinity])
and the functuin is decreasing on the open interval (0,2)\
\>", "Text",
 CellChangeTimes->{{3.819685139648383*^9, 3.819685191494034*^9}, {
  3.819685307311805*^9, 3.8196853744626427`*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["Testing Concave up and down", "Subchapter",
 CellChangeTimes->{{3.819685392378615*^9, 3.819685408933242*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"f", "''"}], "[", 
    RowBox[{"-", "2"}], "]"}], ",", 
   RowBox[{
    RowBox[{"f", "''"}], "[", "3", "]"}]}], "}"}]], "Input",
 CellChangeTimes->{{3.819685423798356*^9, 3.8196854750499067`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"-", "18"}], ",", "12"}], "}"}]], "Output",
 CellChangeTimes->{{3.819685458463857*^9, 3.8196854755313725`*^9}}]
}, Open  ]],

Cell["\<\
Hence the function is concave up on the open intervals  (1,\[Infinity])
and the functuin is concave down on the open interval (-\[Infinity],1)\
\>", "Text",
 CellChangeTimes->{{3.819685497008062*^9, 3.8196855691929445`*^9}}]
}, Open  ]]
}, Open  ]],

